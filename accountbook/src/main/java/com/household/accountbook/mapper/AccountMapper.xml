<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.household.accountbook.mapper.AccountMapper">
    <insert id="accountRegister" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO accounts (name, email, login_id, password) VALUES (#{name}, #{email}, #{loginId}, #{password})
    </insert>
</mapper>